{
  "comments": {
    "lineComment": "# "
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""]
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""]
  ],
  "folding": {
    "offSide": true
  },
  "wordPattern": "[a-zA-Z_][a-zA-Z0-9_]*",
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(let|if|\\[|\\{|\\().*$",
    "decreaseIndentPattern": "^\\s*(]|\\}|\\)).*$"
  }
}
