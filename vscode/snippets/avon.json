{
  "Let Binding": {
    "prefix": "let",
    "body": "let ${1:name} = ${2:value} in",
    "description": "Create a let binding"
  },
  "Lambda Function": {
    "prefix": "lambda",
    "body": "\\\\${1:x} ${2:x}",
    "description": "Create a lambda function"
  },
  "If Conditional": {
    "prefix": "if",
    "body": "if ${1:condition} then ${2:true_value} else ${3:false_value}",
    "description": "Create an if-then-else conditional"
  },
  "Map Function": {
    "prefix": "map",
    "body": "map (\\\\${1:x} ${2:x}) ${3:list}",
    "description": "Map a function over a list"
  },
  "Filter Function": {
    "prefix": "filter",
    "body": "filter (\\\\${1:x} ${2:x > 0}) ${3:list}",
    "description": "Filter a list with a predicate"
  },
  "Args Filter Existing Files": {
    "prefix": "args-filter",
    "body": "args -> filter (\\\\f (exists f))",
    "description": "Filter command line arguments to only existing files"
  },
  "Args Map Process": {
    "prefix": "args-map",
    "body": "args -> map (\\\\${1:arg} ${2:arg})",
    "description": "Map over command line arguments"
  },
  "List": {
    "prefix": "[",
    "body": "[${1:item1}, ${2:item2}]",
    "description": "Create a list"
  },
  "Dictionary": {
    "prefix": "{",
    "body": "{${1:key}: ${2:value}}",
    "description": "Create a dictionary"
  },
  "FileTemplate": {
    "prefix": "file",
  "body": "@${1:path/file.txt} {\"${2:content}\"}",
    "description": "Create a file template"
  },
  "Template String": {
    "prefix": "template",
    "body": "{\"${1:content with {interpolation}}\"}",
    "description": "Create a template string"
  },
  "Import": {
    "prefix": "import",
    "body": "let ${1:module} = import \"${2:path.av}\" in",
    "description": "Import a module"
  },
  "Comments": {
    "prefix": "#",
    "body": "# ${1:comment}",
    "description": "Add a comment"
  },
  "Pipe Operator": {
    "prefix": "->",
    "body": "${1:value} -> ${2:function}",
    "description": "Use pipe operator to chain expressions"
  },
  "Fold Function": {
    "prefix": "fold",
    "body": "fold (\\\\${1:acc} \\\\${2:x} ${3:acc + x}) ${4:initial} ${5:list}",
    "description": "Fold a list into a single value"
  },
  "Range": {
    "prefix": "..",
    "body": "[${1:1}..${2:10}]",
    "description": "Create a range of numbers"
  },
  "Format YAML": {
    "prefix": "format_yaml",
    "body": "format_yaml ${1:value}",
    "description": "Serialize a value to YAML"
  },
  "Format TOML": {
    "prefix": "format_toml",
    "body": "format_toml ${1:value}",
    "description": "Serialize a value to TOML"
  },
  "UUID": {
    "prefix": "uuid",
    "body": "uuid",
    "description": "Generate a random UUID v4"
  },
  "Random Integer": {
    "prefix": "random_int",
    "body": "random_int ${1:min} ${2:max}",
    "description": "Generate a random integer in range [min, max]"
  },
  "Random Float": {
    "prefix": "random_float",
    "body": "random_float ${1:min} ${2:max}",
    "description": "Generate a random float in range [min, max]"
  },
  "Environment Variables": {
    "prefix": "env_vars",
    "body": "env_vars",
    "description": "Get all environment variables as a dictionary"
  },
  "Choice - Random Element": {
    "prefix": "choice",
    "body": "choice ${1:list}",
    "description": "Select a random element from a list"
  },
  "Shuffle List": {
    "prefix": "shuffle",
    "body": "shuffle ${1:list}",
    "description": "Randomly shuffle the elements of a list"
  },
  "Sample Elements": {
    "prefix": "sample",
    "body": "sample ${1:count} ${2:list}",
    "description": "Select n unique random elements from a list"
  },
  "Find Element": {
    "prefix": "find",
    "body": "find (\\\\${1:x} ${2:x > 0}) ${3:list}",
    "description": "Find the first element matching a predicate"
  },
  "Find Index": {
    "prefix": "find_index",
    "body": "find_index (\\\\${1:x} ${2:x > 0}) ${3:list}",
    "description": "Find the index of the first matching element"
  },
  "Group By Key": {
    "prefix": "group_by",
    "body": "group_by (\\\\${1:x} ${2:x % 2}) ${3:list}",
    "description": "Group list elements by a key function"
  },
  "Zip With Function": {
    "prefix": "zip_with",
    "body": "zip_with (\\\\${1:a} \\\\${2:b} ${3:a + b}) ${4:list1} ${5:list2}",
    "description": "Combine two lists using a function"
  },
  "Intersperse Separator": {
    "prefix": "intersperse",
    "body": "intersperse ${1:separator} ${2:list}",
    "description": "Insert a separator between each element of a list"
  }
}
