# Test operator precedence and associativity

let p1 = trace "1 + 2 * 3 (expected 7)" (1 + 2 * 3) in
let p2 = trace "(1 + 2) * 3 (expected 9)" ((1 + 2) * 3) in
let p3 = trace "10 - 5 - 2 (expected 3)" (10 - 5 - 2) in
let p4 = trace "10 / 2 * 5 (expected 25)" (10 / 2 * 5) in
let p5 = trace "100 / 10 / 2 (expected 5)" (100 / 10 / 2) in

# Test negative numbers and operators
let n1 = trace "-5 * 2 (expected -10)" (-5 * 2) in
let n2 = trace "5 * -2 (expected -10)" (5 * -2) in
let n3 = trace "-10 / 2 (expected -5)" (-10 / 2) in
let n4 = trace "10 / -2 (expected -5)" (10 / -2) in

# Test modulo
let m1 = trace "10 % 3 (expected 1)" (10 % 3) in
let m2 = trace "-10 % 3 (expected -1)" (-10 % 3) in
let m3 = trace "10 % -3 (expected 1)" (10 % -3) in
let m4 = trace "-10 % -3 (expected -1)" (-10 % -3) in

