# Test behavior around 0
# 1. 90 -> 110 (Cross 100). Should be 1.
# 2. 100 -> 110 (Start at 0, go Right). Should be 0?
# 3. 100 -> 90 (Start at 0, go Left). Should be 0? (If we don't count leaving)

let test_cross_right = 
  let start = 90 in
  let end = 110 in
  (end / 100) - (start / 100)
in

let test_start_zero_right = 
  let start = 100 in
  let end = 110 in
  (end / 100) - (start / 100)
in

let test_start_zero_left = 
  let start = 100 in
  let end = 90 in
  (start / 100) - (end / 100)
in

let t1 = trace "90 -> 110" test_cross_right in
let t2 = trace "100 -> 110" test_start_zero_right in
let t3 = trace "100 -> 90" test_start_zero_left in
t3
